{
  "$schema": "https://json.schemastore.org/prettierrc",
  "singleQuote": false,
  "semi": true,
  "arrowParens": "always",
  "printWidth": 120,
  "bracketSpacing": true,
  "singleAttributePerLine": true,
  "endOfLine": "lf",
  "trailingComma": "all",
  "overrides": [
    {
      "files": "*.svg",
      "options": {
        "parser": "html"
      }
    },
    {
      "files": ".prettierrc",
      "options": {
        "parser": "json"
      }
    }
  ]
}
